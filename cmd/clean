#!/bin/bash
# deletes generated files to clean out your directory

# ask for confirmation
read -p "This will delete all cli output files. [Y] to continue: " -n 1 -r && echo

# delete files
if [[ $REPLY =~ ^[Yy]$ ]]; then

    # delete data files
    rm -f data/*.bin
    rm -f data/*.csv
    rm -f data/*.npy
    rm -f data/*.pickle
    rm -f data/*.txt
    rm -f data/*.xlsx

    # delete docs
    rm -rfd docs

    # delete logs
    rm -f logs/*.log

    # delete extra configs
    rm -f config/20*

    # delete build files
    rm -rfd build *.egg-info

    # delete azcopy
    rm -f cmd/azcopy
fi
