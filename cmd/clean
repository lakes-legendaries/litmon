#!/bin/bash
# deletes generated files to clean out your directory

# ask for confirmation
read -p "This will delete all cli output files. [Y] to continue: " -n 1 -r && echo

# delete files
if [[ $REPLY =~ ^[Yy]$ ]]; then
    rm -f data/*
    rm -rfd docs
    rm -f logs/*.log
    rm -f config/20*
    rm -rfd build *.egg-info
fi
